<h1 class="text-center">
  <%= @document.title %>
  <button class="btn btn-primary" 
    onclick="verifyIntegrity(&quot;<%= rails_blob_url(@document.doc) %>&quot;, <%= @document.address%>)">
    Verify 
  </button>
</h1>
<div class="image-container center">
  <%= image_tag @document.doc %>
</div>
<a id="manage-perm-btn" href="/documents/<%= @document.id  %>/permissions/new" class="center my-3 px-2 btn btn-info"> Manage Permissions </a>
